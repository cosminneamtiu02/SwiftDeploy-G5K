{
  "running_experiments": {
    "on_fe": {
      "to_do_parameters_list_path": "csnn-ckplus/runs.txt"
    },
    "on_machine": {
      "execute_command": "./Video_3d_CK_Plus_face_single_elypse_experiment",
      "full_path_to_executable": "/root/csnn-build"
    },
    "number_of_experiments_to_run_in_parallel_on_machine": 5,
    "experiments_collection": {
      "base_path": "csnn-ckplus",
      "lookup_rules": [
        { "txt": "*.txt" }
      ],
      "ftransfers": [
        {
          "look_into": "/root/csnn-build/results",
          "look_for": ["txt"],
          "transfer_to_subfolder_of_base_path": "results"
        }
      ]
    }
  },
  "machine_setup": {
    "image_to_use": "env-csnn-st-with-dbs-image.yaml",
    "env_variables_list": [
      { "CK_PLUS_CSV_PATH": "/root/data_CK+/CK+_emotion.csv" },
      { "CK_PLUS_IMAGES_DIR": "/root/data_CK+" }
    ],
    "is_machine_instantiator_manual": true,
    "os_distribution_type": 1
  }
}
